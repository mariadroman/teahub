GET     /projects_setup         controllers.UIController.setup
GET     /project_details        controllers.UIController.details
GET     /profile                controllers.UIController.profile
GET     /new_project            controllers.UIController.newProject
GET     /issues                 controllers.UIController.issues
GET     /user_management        controllers.UIController.management
GET     /callback               controllers.OAuthGitHubController.callback(code: Option[String], state: Option[String])
GET     /success                controllers.OAuthGitHubController.success
GET     /raw_projects           controllers.TEAHubController.githubRepositories
GET     /assets/*file           controllers.Assets.at(path = "/public", file)
